border-radius(val)
  -webkit-border-radius: val
  -moz-border-radius: val
  -ms-border-radius: val
  -o-border-radius: val
  border-radius: val

box-shadow(args...)
   -webkit-box-shadow args
   -moz-box-shadow args
   box-shadow args

/*lender section*/
#lenders
    padding: 20px 0
    border-bottom: 1px solid #eaebea

.slick-slide
  margin: 0px 20px
  vertical-align: middle

.slick-slide img
    margin: 0 auto
    display: block
    max-width: 100%
    height: auto

/*service section*/
#service  , #used-insurance , #protection-insurance , #life-insurance
    padding: 60px 0

#service
  h2
    color: #73af47

.service-widgets
    margin: 40px 0

.service-box
    padding: 30px 20px 20px
    border: 1px solid #ebebeb
    border-radius(4px)
    color: #707070
    strong
      display: block
      margin: 15px 0 5px
      font-size: 17px
      p
          font-size: 15px

.service-box.last
  p
    margin-top: 40px

/*rating section*/
#service
  .rating-box
      background: url(../img/geometry-bg.jpg) no-repeat
      background-size: cover
      background-position: center center
      padding: 40px 40px 40px 10px
      border: 1px solid #ebebeb
      border-radius(4px)
      .carousel-caption
        position: relative
        right: 0
        left: 0
        bottom: 0
        color: #6d6d6d
        text-align: left
      p
        font-size: 16px
      h2
        font-size: 78px
        font-family: 'ProximaNovaLight'
        color: #606060
        span
          font-size: 35px
          vertical-align: top
          margin-top: 10px
          display: inline-block
          margin-left: -15px
      .carousel-indicators
        margin: 0
        bottom: 10px
        .active
          background-color: #73af47
        li
          height: 10px
          max-width: 10px
          border-radius(50%)
          background: #ccc
.rate-name
    font-size: 20px
    font-family: 'ProximaNovaBold'

 .star-rate
    i
      color: #efce4a
      font-size: 24px

.our-rating-holder
    padding-right: 30px

.our-rating
    border-right: 1px solid #ddd

/*compare section*/
#compare-insurance , #benefits-insurance , #protection-insurance
    background-color: #f9f9f9

.compare-box
   padding: 50px 0 50px 50px

.compare-box2
   padding: 80px 60px 80px 0

#compare-insurance
  p
    margin: 25px 0

.powered-list
    padding: 0
    margin-bottom: 10px
    li
      display: block
      font-size: 14px
      position: relative
      padding-left: 40px
      margin-bottom: 15px
    li p
    .powered-list .check-icon
      display: inline-block
    li:before
      content: ''
      position: absolute
      width: 30px
      height: 25px
      border-radius(50%)
      left: 0
      top: 0
      background: url("../img/icons/check.png") no-repeat right center

/*used section*/
#used-insurance .col-md-2
    padding: 30px

.used-icons
    border: 1px solid #ebebeb
    height: 120px
    width: 120px
    border-radius(50%)
    position: relative
    margin: 0 auto 20px
    img
      position: absolute
      left: 0
      right: 0
      top: 0
      bottom: 0
      margin: auto
      display: block

/*protection section*/
.protection-box
    background-color: #fff
    padding: 60px 50px 40px
    position: relative
    box-shadow 0px 5px 15px 0px rgba(000,000,000,0.1)
    min-height: 300px
    img
      position: absolute
      left: 0
      right: 0
      margin: 0 auto
      top: -50px
    p
      margin: 0
    h5
      font-size: 18px

.protection-box-holder
    margin-top: 80px
    margin: 80px 0 40px

/*life section*/
#life-insurance
    img
      position: absolute
      right: 0
      bottom: -60px
    p
      margin: 40px 0 40px
    .powered-list
      padding-right: 40px
    li
      font-weight: 600

/*testimonial section*/
#testimonial-insurance
    padding: 40px 0 80px
    background: url(../img/testimonial-bg.jpg) no-repeat
    background-size: cover
    .carousel-caption
      position: relative
      right: 0
      left: 0
      bottom: 0
      color: #6d6d6d
      text-align: left
    .carousel-item
      background: #fff
      padding: 15px 40px 0px
    .carousel-inner
      width: 90%
      margin: 30px auto 0
      box-shadow 0px 5px 15px 5px rgba(16,16,16,0.1)
    .carousel-control-next-icon
    .carousel-control-prev-icon
      width: 35px
      height: 35px
    .carousel-control-next
    .carousel-control-prev
      width: 5%
      opacity: 1
    .carousel-control-prev-icon
      background-image: url(../img/icons/prev-icon.png)
    .carousel-control-next-icon
      background-image: url(../img/icons/next-icon.png)
    .carousel-indicators li
      height: 10px
      max-width: 10px
      border-radius(50%)
      background: #ccc
    .carousel-indicators .active
      background: #69a646
    .carousel-indicators
      margin: 0
      bottom: -40px
    .rate-name
      font-size: 18px
      color: #f39c12
      text-align: right
      margin: 0

/*blog section*/
#blog-insurance
    padding: 40px 0
    clear: both

#blog-box
    margin: 40px 0
    img
      width: 100%

.content-post
    padding: 100px 25px 35px 25px
    color: #fff
    text-align: left

.post-date
    font-weight: bold
.post-content
    background-color: #fff
    padding: 20px
    border-top: 3px solid #96ca4b
    position: relative
    p
      color: #424242
      font-size: 14px
      font-weight: 600
    .advice-text
      color: #848484
      margin-bottom: 10px

.post-content:after
    font-size: 0px
    line-height: 0%
    width: 0px
    border-top: 20px solid #fff
    border-left: 10px solid transparent
    border-right: 10px solid transparent
    content: ""
    display: block
    position: absolute
    bottom: -10px
    left: 30px

.full-blog-sec
    margin-top: 25px

.date
    font-size: 14px

#featured-bg-1
    height: 100%
    position: relative
    background: url(../img/blog-img1.jpg) no-repeat
    background-size: cover
    background-position: center center
    .content-post
      position: absolute
      bottom: 0;

#featured-bg-2
    background: url(../img/blog-img2.jpg) no-repeat
    background-size: cover
    background-position: center center

#featured-bg-3
    background: url(../img/blog-img3.jpg) no-repeat
    background-size: cover
    background-position: center center

#featured-bg-4
    background: url(../img/blog-img4.jpg) no-repeat
    background-size: cover
    background-position: center center
    .post-content
      width: 44%

/*policies section */
#policies-insurance
    padding: 35px 0
    background: url(../img/compare-bg.jpg) no-repeat
    background-size: cover
    h3
      font-size: 28px
      color: #fff
      margin-right: 40px
      display: inline-block
    .btn
      display: inline-block

/*inner section*/
#inner-page
    position: relative
    padding: 100px 0 60px
    background-color: #fafafa
    .inner-main-title:after
      background: linear-gradient(140deg, #75a245 0%, #71a145 50%, #538842 75%)
      width: 170px
      height: 6px
      display: block
      content: ""
      margin-top: 10px

#inner-content
    .container
      background-color: #fff
      padding: 70px 80px
      box-shadow 0px 0px 4px 0px rgba(70,70,70,0.1)

#inner-page h2
    font-size: 32px
    font-family: 'ProximaNovaBold'

/*archive section*/
.post-title
    color: #292929

.blog-post
    padding: 30px 0
    border-bottom: 1px solid #e0e0e0
    .meta
      margin-bottom: 15px
    .excerpt
      text-align: justify
    .post-date
      margin-right: 20px
    .post-date:before
    .post-author:before
      display: inline-block
      content: ""
      height: 20px
      width: 20px
      position: relative
      top: 5px
      margin-right: 5px
    .post-date:before
      background: url(../img/icons/post-calendar-icon.png) no-repeat

    .post-author:before
      background: url(../img/icons/post-author-icon.png) no-repeat

.blog-post img
.blog-single-post img
    margin-bottom: 20px
    width: 100%

.blog-post .btn
.blog-single-post .btn
    font-size: 16px

.blog-single-post
    p
      margin-bottom: 30px
    .meta
      margin-bottom: 15px
    .btn
      margin-top: 20px
    .post-date
      margin-right: 20px

#pagination
    margin: 30px 0 0
    padding: 0
    text-align: center
    li
      display: inline-block
      margin-right: 10px
      position: relative
      a
        border: 1px solid #6eab47
        border-radius(50%)
        font-size: 15px
        color: #777
        width: 34px
        height: 34px
        text-align: center
        font-family: 'ProximaNovaSimibold'
      .dot
        border: none
        background-size: none
        padding: 0;
      .dot:hover
        background: none
        color: #777
      .next
        background: #dedede
        border: none
      .next:hover
        background: #f2f2f2
      .next:before
        width: 0px
        content: ""
        display: block
        border-top: 6px solid transparent
        border-left: 10px solid #939393
        border-bottom: 6px solid transparent
        position: absolute
        left: 0
        right: 0
        margin: 4px auto 0
      .active
      a:hover
        background: linear-gradient(140deg, #75a245 0%, #71a145 50%, #538842 75%)
        color: #fff

.blog-single-post
    .post-date:before
    .post-author:before
      display: inline-block
      content: ""
      height: 20px
      width: 20px
      position: relative
      top: 5px
      margin-right: 5px
    .post-date:before
      background: url(../img/icons/post-calendar-icon.png) no-repeat
    .post-author:before
      background: url(../img/icons/post-author-icon.png) no-repeat
